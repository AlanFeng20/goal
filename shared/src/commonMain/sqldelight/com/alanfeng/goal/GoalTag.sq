CREATE TABLE GoalTagEntity (
    id Integer  PRIMARY KEY AUTOINCREMENT ,
    goal_id INTEGER NOT NULL,
    text Text NOT NULL ,
    FOREIGN KEY (goal_id) REFERENCES GoalEntity(id) ON DELETE CASCADE ON UPDATE CASCADE
);

insertGoalTag:
INSERT OR IGNORE INTO GoalTagEntity (goal_id, text) VALUES (goal_id,text);

selectByGoalId:
SELECT * FROM GoalTagEntity WHERE goal_id = ?;

selectAll:
SELECT * FROM GoalTagEntity;

deleteByGoalId:
DELETE FROM GoalTagEntity WHERE goal_id = ?;

